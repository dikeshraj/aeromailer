{
  "name": "dikeshraj/aeromailer",
  "description": "AeroMailer custom Laravel mail driver",
  "type": "library",
  "autoload": {
    "psr-4": {
      "YourCompany\\AeroMailer\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/mail": "^10.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "DikeshRajGiri\\AeroMailer\\AeroMailerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
