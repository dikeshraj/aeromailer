{
  "name": "dikeshraj/aeromailer",
  "description": "AeroMailer - client Laravel mail driver sending via central AeroMailer API",
  "type": "library",
  "license": "proprietary",
  "authors": [
    { "name": "Dikesh Raj Giri", "email": "giridikesh03@gmail.com" }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/mail": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "DikeshRaj\\AeroMailer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DikeshRaj\\AeroMailer\\Providers\\AeroMailerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
