{
  "name": "dikeshraj/aeromailer",
  "description": "AeroMailer custom Laravel mail driver (client package)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^12.0",
    "illuminate/mail": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "DikeshRajGiri\\AeroMailer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DikeshRajGiri\\AeroMailer\\AeroMailerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
